<Window x:Class="BENYEMNA_BURY_B.RechercheClient"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BENYEMNA_BURY_B"
        mc:Ignorable="d"
        Title="RechercheClient" Height="450" Width="800">
    <Grid>
        <Button x:Name="Rechercher" Content="Rechercher" HorizontalAlignment="Left" VerticalAlignment="Top" Width="78" Margin="431,155,0,0" Click="RechercheParNum" Height="20"/>
        <Button x:Name="NouveauClient" Content="Nouveau Client" HorizontalAlignment="Left" Margin="312,231,0,0" VerticalAlignment="Top" Width="100" Height="20" Click="Ajouter"/>
        <Button x:Name="Retour" Content="Retour" HorizontalAlignment="Left" Margin="10,390,0,0" VerticalAlignment="Top" Width="75" Height="20" Click="RetourAccueil"/>
        <TextBox x:Name="Telephone" HorizontalAlignment="Left" Height="20" Margin="290,155,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBlock HorizontalAlignment="Left" Margin="148,155,0,0" TextWrapping="Wrap" Text="Numero de telephone: " VerticalAlignment="Top" Height="20" Width="125"/>

    </Grid>
</Window>
